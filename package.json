	"dependencies": {
		"basic-ftp": "^5.0.5",
		"mqtt": "^5.13.3",
		"tsc-alias": "^1.8.16"
	},
	"devDependencies": {
		"@types/node": "^24.1.0",
		"dotenv": "^17.2.1",
		"prettier": "^3.6.2",
		"tsx": "^4.20.3",
		"typescript": "^5.8.3"
	},
	"packageManager": "pnpm@9.7.0"
