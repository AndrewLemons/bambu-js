{
	"name": "bambu-js",
	"version": "3.0.0",
	"description": "Tools to interact with Bambu Lab printers",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.js"
		}
	},
	"type": "module",
	"keywords": [
		"bambu",
		"printer",
		"mqtt",
		"ftp",
		"camera"
	],
	"author": {
		"name": "Andrew Lemons",
		"url": "https://github.com/AndrewLemons"
	},
	"license": "MIT",
	"scripts": {
